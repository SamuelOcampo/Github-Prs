{
  "manifest_version": 3,
  "name": "Github PRs",
  "description": "PRs can't wait",
  "version": "0.0.1",
  "icons": {
    "16": "assets/16.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "src/scripts/background.ts",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "src/static/new-tab.html"
  }
}
